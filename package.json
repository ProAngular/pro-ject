{
  "name": "pro-ject",
  "version": "1.0.0",
  "description": "A lightweight Node CLI that guides `ng new` and scaffolds an Angular 20+ workspace with opinionated structure and best-practice defaults.",
  "bin": {
    "pro-ject": "dist/index.js"
  },
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsc && node .scripts/copy-files.mjs",
    "start": "npm run build && node dist/index.js",
    "dev": "tsx src/index.ts"
  },
  "dependencies": {
    "execa": "^9.4.0",
    "prompts": "^2.4.2",
    "validate-npm-package-name": "^5.0.0"
  },
  "devDependencies": {
    "@types/prompts": "^2.4.9",
    "@types/validate-npm-package-name": "^4.0.2",
    "prettier": "^3.6.2",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  },
  "keywords": [
    "angular",
    "cli",
    "wizard",
    "scaffold",
    "ng"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
